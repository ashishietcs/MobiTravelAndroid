<?xml version="1.0" encoding="utf-8"?>

<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="org.smartcity"
    android:versionCode="1"
    android:versionName="1.0" >

    
    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />



    <uses-sdk
        android:minSdkVersion="8"
        android:targetSdkVersion="21" />

<application
        android:allowBackup="true"
        android:icon="@drawable/ic_launcher"
        android:label="Login">
        <activity android:name="org.smartcity.Login"  android:label="Login" ></activity>

        <activity android:name="org.smartcity.TicketCheck" ></activity>
        <activity android:name="org.smartcity.WelcomeScreen" android:label="Welcome" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name="org.smartcity.TicketBooking" ></activity>
        <activity android:name="org.smartcity.TicketPrint" ></activity>

</application>
</manifest>
